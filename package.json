{
  "name": "solidityplayground",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json' && typechain --target ethers-v5 'build/contracts/*.json' --out-dir web/src/contracts",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "typecheck": "tsc --noEmit",
    "test": "truffle test",
    "compile": "truffle compile && npm run generate-types",
    "clean": "rm -rf migrations && rm -rf build && rm -rf types && rm -rf web/src/contracts"
  },
  "repository": {
    "type": "git",
    "url": "git@ssh-gitlab.rumblefishdev.com:daniel.cybulka/solidityplayground.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/truffle-v5": "^5.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/web3": "^1.2.2",
    "dotenv": "^10.0.0",
    "truffle": "^5.3.8",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-typings": "^1.0.8",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2",
    "web3": "^1.3.6"
  },
  "dependencies": {
    "ethers": "^5.3.1",
    "use-metamask": "^1.1.2"
  }
}